<h1>Sign In</h1>
<br>
<%= render 'shared/flash' %>
<%= simple_form_for :session, url: sessions_path do |f| %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :password %>
  </div>
  <br>
  <div class="form-actions">
    <%= f.button :submit, 'Sign in' %>
  </div>
<% end %> 
<p>New user? <%= link_to "Sign up now!", new_user_path %></p>
